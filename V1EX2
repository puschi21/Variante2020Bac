#include <iostream>

using namespace std;

int main()
{
    int n,k,i,j,l;
    cin>>n>>k;
    for(i=1;i<=n;i++)
    {
        for(j=i;j<=n+i-1;j++)
        {
            l=k;
            while(l)
            {
                cout<<j<<" ";
                l--;
            }
        }
        cout<<endl;
    }
    return 0;
}
